{
  "name": "nl-compiler",
  "displayName": "nl-compiler",
  "description": "newsletter compiler",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:html"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nl-compiler.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.29.0",
    "globals": "^16.2.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "clean-css": "^5.3.3",
    "css": "^3.0.0",
    "js-beautify": "^1.15.4",
    "postcss": "^8.5.6",
    "punycode": "^2.3.1"
  }
}
